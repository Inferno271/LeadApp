<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    tools:ignore="InvalidNavigation">

    <fragment
        android:id="@+id/contactListFragment"
        android:name="com.inferno271.leadapp15.ContactListFragment"
        android:label="Contact List"
        tools:layout="@layout/fragment_contact_list" />

    <fragment
        android:id="@+id/contactDetailFragment"
        android:name="com.inferno271.leadapp15.ContactDetailFragment"
        android:label="Contact Detail"
        tools:layout="@layout/fragment_contact_detail" />

    <!-- Добавьте другие фрагменты, если необходимо -->
    <action
        android:id="@+id/action_contactListFragment_to_contactDetailFragment"
        app:destination="@id/contactDetailFragment" />

</navigation>
